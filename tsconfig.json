{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    /* Module Resolution Options */
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es2019",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "incremental": true,
    // "paths": {
    //   "../../src/config*": ["cosrc/nfig/*"],
    //   "../../src/common/*": ["../../src/common/*"],
    //   "../../src/constant/*": ["src/constant/*"],
    //   "../../src/validators/*": ["src/validators/*"],
    //   "../../src/utils/*": ["src/utils/*"],
    //   "../../src/middlewares/*": ["src/middlewares/*"],
    //   "../../src/*": ["src/*"]
    // }
  },
  "lib": ["ES2019"]
}
